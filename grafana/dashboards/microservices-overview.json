{
    "annotations": {
        "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "id": null,
    "links": [],
    "panels": [
        {
            "title": "Request Rate (req/s)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "targets": [
                {
                    "expr": "rate(http_request_duration_seconds_count[1m])",
                    "legendFormat": "{{job}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "fillOpacity": 20,
                        "lineWidth": 2,
                        "showPoints": "never"
                    },
                    "unit": "reqps"
                },
                "overrides": []
            }
        },
        {
            "title": "Error Rate (5xx)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "targets": [
                {
                    "expr": "rate(http_request_duration_seconds_count{status_code=~\"5..\"}[1m])",
                    "legendFormat": "{{job}} 5xx",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "fixed",
                        "fixedColor": "red"
                    },
                    "custom": {
                        "fillOpacity": 30,
                        "lineWidth": 2,
                        "showPoints": "never"
                    },
                    "unit": "reqps"
                },
                "overrides": []
            }
        },
        {
            "title": "P95 Response Latency",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
                    "legendFormat": "{{job}} p95",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "fillOpacity": 10,
                        "lineWidth": 2,
                        "showPoints": "never"
                    },
                    "unit": "s"
                },
                "overrides": []
            }
        },
        {
            "title": "Cache Hit vs Miss (Task Service)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 8
            },
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "targets": [
                {
                    "expr": "rate(cache_hits_total[1m])",
                    "legendFormat": "Cache Hits",
                    "refId": "A"
                },
                {
                    "expr": "rate(cache_misses_total[1m])",
                    "legendFormat": "Cache Misses",
                    "refId": "B"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "fillOpacity": 25,
                        "lineWidth": 2,
                        "showPoints": "never"
                    },
                    "unit": "ops"
                },
                "overrides": [
                    {
                        "matcher": {
                            "id": "byName",
                            "options": "Cache Hits"
                        },
                        "properties": [
                            {
                                "id": "color",
                                "value": {
                                    "mode": "fixed",
                                    "fixedColor": "green"
                                }
                            }
                        ]
                    },
                    {
                        "matcher": {
                            "id": "byName",
                            "options": "Cache Misses"
                        },
                        "properties": [
                            {
                                "id": "color",
                                "value": {
                                    "mode": "fixed",
                                    "fixedColor": "orange"
                                }
                            }
                        ]
                    }
                ]
            }
        },
        {
            "title": "RabbitMQ Events Consumed",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 0,
                "y": 16
            },
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "targets": [
                {
                    "expr": "sum(events_consumed_total)",
                    "legendFormat": "Total Consumed",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "fixed",
                        "fixedColor": "purple"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 50
                            },
                            {
                                "color": "red",
                                "value": 100
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
            "title": "RabbitMQ Events Published",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 6,
                "y": 16
            },
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "targets": [
                {
                    "expr": "sum(events_published_total)",
                    "legendFormat": "Total Published",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "fixed",
                        "fixedColor": "blue"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 50
                            },
                            {
                                "color": "red",
                                "value": 100
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
            "title": "Service Health",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 12,
                "x": 12,
                "y": 16
            },
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "targets": [
                {
                    "expr": "up",
                    "legendFormat": "{{job}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "mappings": [
                        {
                            "type": "value",
                            "options": {
                                "0": {
                                    "text": "DOWN",
                                    "color": "red"
                                },
                                "1": {
                                    "text": "UP",
                                    "color": "green"
                                }
                            }
                        }
                    ],
                    "thresholds": {
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "green",
                                "value": 1
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
